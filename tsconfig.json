{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "src/*": ["src/*"],
      "assets/*": ["src/assets/*"],
      "images/*": ["src/assets/images/*"],
      "components/*": ["src/components/*"],
      "constants/*": ["src/constants/*"],
      "context/*": ["src/context/*"],
      "hooks/*": ["src/hooks/*"],
      "navigation/*": ["src/navigation/*"],
      "screens/*": ["src/screens/*"],
      "utils/*": ["src/utils/*"]
    }
  }
}
